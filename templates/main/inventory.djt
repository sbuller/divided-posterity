<html>
	<head>
		<title>Inventory</title>
	</head>
	<body>
		<h1>Inventory</h1>
		{% if items %}
			<ul>
			{% for item in items %}
				<li>{{item.item.name}} <b>x</b>{{item.quantity}}</li>
			{% endfor %}
			</ul>
		{% else %}
			<p> Error: No items available at this time. </p>
		{% endif %}
		<a href="/">Index</a>
	</body>
</html>
