<html>
	<head>
		<title>A Title</title>
		<meta http-equiv="Content-Script-Type" content="text/javascript">
	</head>
	<body>
		<h1>The Map</h1>
		<h2>You are at {{location.name}}</h2>

		<table><tbody>
			<tr>
				<td>empty</td>
				<td>
					{% if places.witchs_cottage %}
						<a href="/map/{{places.witchs_cottage.slug}}">{{places.witchs_cottage.name}} </a>
					{% else %}
						empty
					{% endif %}
				</td>
				<td>empty</td>
			</tr>
			<tr>
				<td>empty</td>
				<td><a href="/map/{{location.slug}}">Fight Something!</a></td>
				<td>
					{% if places.forest %}
						<a href="/map/{{places.forest.slug}}">{{places.forest.name}}</a>
					{% else %}
						empty
					{% endif %}
				</td>
			</tr>
			<tr>
				<td>empty</td>
				<td>empty</td>
				<td>
					{% if places.yerts_hut %}
						<a href="/map/{{places.yerts_hut.slug}}"> {{places.yerts_hut.name}}</a>
					{% else %}
						empty
					{% endif %}
				</td>
			</tr>
		</tbody></table>
		<a href="/">Back to index</a>
	</body>
</html>
